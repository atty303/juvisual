<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ja" lang="ja">
  <head>
    <title></title>
    <link rel="stylesheet" href="/static/base.css" type="text/css" media="screen" />
  </head>
  <body>
    <h1>{{ message|e }}</h1>

    <table class="mulist">
      <tbody>
        {% for s in scores %}
        <tr>
          <th class="left"></th>
          <th class="title">{{ s.title }}</th>
          <td class="level"><div class="level1">{{ s.level_bas }}</div></td>
          <td class="data">
            <table class="mb"><tbody>
              <tr class="detail">
                <td class="ind ind_ng"><span class="ind_ng">NG</span></td>
                <td class="ind ind_ay"><span class="ind_ay">AY</span></td>
                <td class="ind ind_fc"><span class="ind_fc">FC</span></td>
                <td class="rating"><span class="rating_exc">EXC</span></td>
                <td class="score">{{ s.score_bas }}</td>
                <td></td>
              </tr>
              <tr class="mb_notes"><td colspan="6">
                {% for c in s.mb_bas -%}
                {%- if c == 1 -%}
                <span class="mbg"></span>
                {%- elif c == 2 -%}
                <span class="mbb"></span>
                {%- elif c == 3 -%}
                <span class="mby"></span>
                {%- endif -%}
                {%- endfor %}
              </td></tr>
            </tbody></table>
          </td>
          <th class="right"></th>
        </tr>
        {% endfor %}
      </tbody>
    </table>

    <form method="post" action="/regist" enctype="multipart/form-data">
      <p>
        <input type="file" name="score_record_file" />
        <input type="submit" name="submit" value="submit" />
      </p>
    </form>
  </body>
</html>
